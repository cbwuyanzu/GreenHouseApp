<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cddddzzzz.greenhousetongji.MainActivity"
    tools:showIn="@layout/app_bar_main">

    <TextView
        android:id="@+id/Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Outdoor" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/Out"
        android:padding="10dp"
        android:text="Temperature:" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView3"
        android:padding="10dp"
        android:text="Humidity:" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:padding="10dp"
        android:text="Radiation:" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView4"
        android:padding="10dp"
        android:text="CO2 Concentration:" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/textView5"
        android:padding="10dp"
        android:text="Wind direction:" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView6"
        android:padding="10dp"
        android:text="Wind Speed:" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView7"
        android:padding="10dp"
        android:text="Rain:" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView8"
        android:padding="10dp"
        android:text="Atmosphere:" />
    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView9"
        android:padding="10dp"
        android:text="Update time:" />

    <TextView
        android:id="@+id/out_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView3"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="temp" />

    <TextView
        android:id="@+id/out_hum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView2"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="hum" />

    <TextView
        android:id="@+id/out_rad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView4"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="rad" />

    <TextView
        android:id="@+id/out_co2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView5"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="CO2" />

    <TextView
        android:id="@+id/out_wind_dir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView6"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="direction" />

    <TextView
        android:id="@+id/out_wind_speed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView7"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="speed" />

    <TextView
        android:id="@+id/out_rain"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView8"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="rain" />

    <TextView
        android:id="@+id/out_atmos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView9"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="atomosphere" />
    <TextView
        android:id="@+id/out_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView10"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="update" />
</RelativeLayout>
