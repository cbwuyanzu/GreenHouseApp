<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="cddddzzzz.greenhousetongji.IndoorActivity"
    tools:showIn="@layout/app_bar_indoor">

    <TextView
        android:id="@+id/In"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="Indoor" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/In"
        android:padding="10dp"
        android:text="Temperature:" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:padding="10dp"
        android:text="Humidity:" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView12"
        android:padding="10dp"
        android:text="Radiation:" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:padding="10dp"
        android:text="CO2 Concentration:" />
    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:padding="10dp"
        android:text="Update time:" />

    <TextView
        android:id="@+id/in_temp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView11"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="temp" />

    <TextView
        android:id="@+id/in_hum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView12"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="hum" />

    <TextView
        android:id="@+id/in_rad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView13"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="rad" />

    <TextView
        android:id="@+id/in_co2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView14"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="CO2" />

    <TextView
        android:id="@+id/in_update"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@id/textView15"
        android:layout_centerHorizontal="true"
        android:padding="10dp"
        android:text="update" />
</RelativeLayout>
